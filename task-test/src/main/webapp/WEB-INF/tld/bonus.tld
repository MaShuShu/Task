<?xml version="1.0" encoding="UTF-8" ?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"
	version="2.0">

	<description>Bonus util Functions</description>
	<tlib-version>1.0</tlib-version>
	<short-name>bonus</short-name>
	<uri>/tld/bonusFunction</uri>
	<function>
		<name>getCityName</name>
		<function-class>com.peaceful.crm.util.BonusTldUtil</function-class>
		<function-signature>java.lang.String getCityName(java.lang.String)</function-signature>
		<example>${bonus.getCityName('')}</example>
	</function>
	
	<function>
		<name>formatDateTime</name>
		<function-class>com.peaceful.crm.util.BonusTldUtil</function-class>
		<function-signature>java.lang.String formatDateTime(java.util.Date)</function-signature>
		<example>${bonus.formatDateTime(new java.util.Date())}</example>
	</function>
	
	<function>
		<name>formatDate</name>
		<function-class>com.peaceful.crm.util.BonusTldUtil</function-class>
		<function-signature>java.lang.String formatDate(java.util.Date)</function-signature>
		<example>${bonus.formatDate(new java.util.Date())}</example>
	</function>
	
	<function>
		<name>formatTime</name>
		<function-class>com.peaceful.crm.util.BonusTldUtil</function-class>
		<function-signature>java.lang.String formatTime(java.util.Date)</function-signature>
		<example>${bonus.formatTime(new java.util.Date())}</example>
	</function>
	
	
	<function>
		<name>getBonusCode</name>
		<function-class>com.peaceful.crm.util.BonusTldUtil</function-class>
		<function-signature>java.lang.Object getBonusCode(java.lang.String)</function-signature>
		<example>${bonus.getBonusCode("")}</example>
	</function>
	
	<function>
		<name>getBonusType</name>
		<function-class>com.peaceful.crm.util.BonusTldUtil</function-class>
		<function-signature>java.lang.String getBonusType(java.lang.Integer)</function-signature>
		<example>${bonus.getBonusType(1)}</example>
	</function>
	
	<function>
		<name>getDimensionName</name>
		<function-class>com.peaceful.crm.util.BonusTldUtil</function-class>
		<function-signature>java.lang.String getDimensionName(java.lang.Integer,java.lang.Integer)</function-signature>
		<example>${bonus.getDimensionName(1,1)}</example>
	</function>
	<function>
		<name>collectionContains</name>
		<function-class>com.peaceful.crm.util.BonusTldUtil</function-class>
		<function-signature>java.lang.Boolean collectionContains(java.lang.Object,java.lang.Object)</function-signature>
		<example>${bonus.collectionContains(list,str)}</example>
	</function>
	
</taglib>
   